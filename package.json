{
  "name": "serverless-aws-boilerplate",
  "version": "0.0.0",
  "main": "index.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "license": "MIT",
  "scripts": {
    "build": "serverless package",
    "deploy": "serverless deploy --verbose --stage staging",
    "deploy-production": "yarn test-production && yn && serverless deploy --verbose --stage production",
    "invoke": "serverless invoke local --function hello",
    "logs": "serverless logs --tail --stage staging --function hello",
    "test": "serverless invoke test --compilers js:babel-core/register",
    "test-production": "serverless invoke test --stage production --compilers js:babel-core/register"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "serverless": "^1.25.0",
    "serverless-mocha-plugin": "^1.7.0",
    "serverless-plugin-optimize": "^3.0.4-rc.1",
    "webpack": "^3.10.0",
    "yn-cli": "^1.0.0"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "middy": "^0.5.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
